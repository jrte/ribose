intervals = (
	'<cycle-start id="' digit+ '" type="scavenge" contextid="' digit+ '" timestamp="' (digit+ ('-T:.':alph))+ digit+ '" intervalms='
	('"', select[`~interval`]) (digit, paste)+ ('.', paste) (digit, paste)+ ('"', out[`~interval` nl] clear[`~interval`])
	(utf7 - (nl | '>'))* '>'
)*;

l0 = (intervals$0):alph;
l1 = (intervals$1):alph;
l2 = (intervals$2):alph;

intervalsnul = (
	(
			((NotNlOrNul* @ intervals)$(0 1 2))
		@	((l0$(0,0))* (l1$(0,,0))* (l2$(0,,,0))*)* (nul$(0,0)) (nul* l0* l1* l2*)* 
	)
	(nul* notnl*)* nl
);

Tintervals = (
	(intervalsnul)*
	intervals
)*
(intervalsnul)*;

Tintervals$(0,1 2):prsseq `Tintervals.pr`;
Tintervals:save `Tintervals.dfa`;
Tintervals$0:alph:card;

_intervals = (
	'<cycle-start id="' digit+ '" type="scavenge"' (any* - 'intervalms=')* 'intervalms='
	('"', select[`~interval`]) (digit, paste)+ ('.', paste) (digit, paste)+ ('"', out[`~interval` nl] clear[`~interval`])
	(utf7 - (nl | '>'))* '>'
)*;

l0 = (_intervals$0):alph;
l1 = (_intervals$1):alph;
l2 = (_intervals$2):alph;

_intervalsnul = (
	(
			((NotNlOrNul* @ _intervals)$(0 1 2))
		@	((l0$(0,0))* (l1$(0,,0))* (l2$(0,,,0))*)* (nul$(0,0)) (nul* l0* l1* l2*)* 
	)
	(nul* notnl*)* nl
);

Sintervals = (
	(_intervalsnul)*
	_intervals
)*
(_intervalsnul)*;

Sintervals$(0,1 2):prsseq `Sintervals.pr`;
Sintervals:save `Sintervals.dfa`;
Sintervals$0:alph:card;
