# common names for printable 7-bit characters and character sets
lower = letters$0;
upper = letters$1;
digit = '0123456789':alph;
punct = '!\"#$%&\'()*+,-./:;<=>?@[\\]^_`{|}~':alph;
space = ' ';
comma = ',';
dot = '.';
dash = '-';
pipe = '|';
